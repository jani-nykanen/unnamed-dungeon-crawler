{
    "compilerOptions": {
        "target": "es6",
        "lib": ["ES2016", "DOM"],
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outFile": "./out.js",
        "sourceRoot": "./src/",
        "rootDir": "./"
    },
    "include": [
        "src/core/*",
        "src/*"
    ],
}
